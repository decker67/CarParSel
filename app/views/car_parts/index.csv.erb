<%- headers = ['Matchcode', 'Lagerhaltungsnummer', 'Bezeichnung', 'Vk1', 'Bestand', 'VKK',
               'Bild 1', 'eBay Kategorie 1', 'eBay Shop Kategorie 1', 'eBay Shop Kategorie 2',
               'Benutzerdef. Merkmale 1', 'Benutzerdef. Merkmale 2', 'HTML', 'Bezeichnung (eBay)',
               'Notiz', 'Sofort-kaufen Preis (eBay)', 'Erstellungsdatum', 'Bei eBay einstellen',
               'Zustand', '	Währung Stammdaten',  'Währung eBay-Daten', 'MwSt Satz',
               'eBay Marktplatz'] -%>
<%= CSV.generate_line headers %>
<%- @car_parts.each do |car_part| -%>
    <%= CSV.generate_line([car_part.id,
                           car_part.stock,
                           car_part.description,
                           car_part.price,
                           car_part.quantity,
                           'versand',
                           absolute_picture_url(car_part.car, car_part.picture_url1),
                           car_part.ebay_shop_category,
                           car_part.car.ebay_shop_category,
                           car_part.car.ebay_second_shop_category,
                           'Teilernummer:' + car_part.part_number,
                           create_car_details(car_part.car),
                           'HTML',
                           car_part.description,
                           car_part.remark,
                           car_part.price,
                           car_part.created_at,
                           car_part.ebay_state == 1 ? 'ja' : 'nein',
                           3000,
                           'EUR',
                           'EUR',
                           '19',
                           'DE']).html_safe %>
<%- end -%>